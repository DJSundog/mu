--regex-mu=/^recipe![ \t]+([^ \t]*)/\1/d,definition/
--regex-mu=/^container[ \t]+([^ \t]*)/\1/s,struct/
--regex-mu=/^exclusive-container[ \t]+([^ \t]*)/\1/u,union/
--regex-mu=/^[ \t]*(<[^ \t]*>)/\1/d,definition/
