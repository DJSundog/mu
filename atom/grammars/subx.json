{
    "scopeName": "source.subx",
    "name": "SubX",
    "fileTypes": ["subx"],
    "patterns": [
        {
            "name": "comment.line.subx",
            "match": "#.*"
        },
        {
            "name": "test.subx",
            "match": "^test-[^ ]*:"
        },
        {
            "name": "global.subx",
            "match": "^[A-Z][^ ]*:"
        },
        {
            "name": "function.subx",
            "match": "^[^_$ #][^ ]*:"
        },
        {
            "name": "label.subx",
            "match": "^[_$][^ ]*:"
        }
    ]
}
